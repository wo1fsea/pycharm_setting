import sys
import time

from PyQt5 import QtCore, QtGui
from PyQt5.QtCore import pyqtSignal, QPoint, QRect, QSize
from PyQt5.QtWidgets import QApplication, QMainWindow, QLabel, QWidget, QGraphicsView, QRubberBand, QGraphicsScene


class ScreenGrabber(QLabel):
    def __init__(self, parent):
        self._parent = parent
        super(ScreenGrabber, self).__init__(None)
        self.rubberBand = QRubberBand(QRubberBand.Rectangle, self)
        self.rubberBand.show()
        self.rubberBand.setGeometry(0, 0, 0, 0)
        self.origin = QPoint()
        self.changeRubberBand = False
        self.setCursor(QtCore.Qt.CrossCursor)

        temp_screen_num = len(QApplication.screens())
        desktopRect = QApplication.desktop().rect()
        if temp_screen_num != 1:
            temp = desktopRect.width() - (desktopRect.width() / temp_screen_num)
            desktopRect = QtCore.QRect(-temp, 0, desktopRect.width(), desktopRect.height())

        pqscreen = QApplication.primaryScreen()
        pixmap0 = pqscreen.grabWindow(0, desktopRect.x(), desktopRect.y(), desktopRect.width(), desktopRect.height())
        pixmap = QtGui.QPixmap(pixmap0)
        pixmap1 = QtGui.QPixmap(pixmap.width(), pixmap.height())
        pixmap1.fill((QtGui.QColor(0, 0, 0, 128)))
        painter = QtGui.QPainter()
        painter.begin(pixmap)
        painter.drawPixmap(0, 0, pixmap1)
        painter.end()

        self._pixmap = pixmap
        self.setPixmap(pixmap)
        self.setScaledContents(True)
        self.setAlignment(QtCore.Qt.AlignCenter)
        self.resize(pixmap.width(), pixmap.height())
        self.setGeometry(desktopRect)
        self.setWindowFlags(QtCore.Qt.WindowStaysOnTopHint)
        self.showFullScreen()

    def mouseDoubleClickEvent(self, event):
        if event.button() != QtCore.Qt.LeftButton:
            return

        self.setParent(self._parent)
        rect = self.rubberBand.geometry()
        rect.setTop(rect.top() * self._pixmap.width() / self.width())
        rect.setLeft(rect.left() * self._pixmap.height() / self.height())
        rect.setBottom(rect.bottom() * self._pixmap.width() / self.width())
        rect.setRight(rect.right() * self._pixmap.height() / self.height())
        self._pixmap.copy(rect).save(&quot;a.png&quot;)
        self.close()

    def mousePressEvent(self, event):
        super(ScreenGrabber, self).mousePressEvent(event)
        if event.button() != QtCore.Qt.LeftButton:
            return

        if not self.rubberBand.geometry().isEmpty():
            return

        self.origin = event.pos()
        self.rubberBand.setGeometry(QRect(self.origin, QSize()))
        self.rubberBand.show()
        self.changeRubberBand = True
        # QLabel.mousePressEvent(self, event)

    def mouseMoveEvent(self, event):
        super(ScreenGrabber, self).mouseMoveEvent(event)
        if self.changeRubberBand:
            self.rubberBand.setGeometry(QRect(self.origin, event.pos()).normalized())
        # QLabel.mouseMoveEvent(self, event)

    def mouseReleaseEvent(self, event):
        super(ScreenGrabber, self).mouseReleaseEvent(event)
        self.changeRubberBand = False
        print(self.rubberBand.geometry().size())


class App(QMainWindow):
    def __init__(self):
        super(App, self).__init__()
        self.title = 'monitor'
        self.createsASubwindow()

    def createsASubwindow(self):
        self.mySubwindow = ScreenGrabber(self)
        self.show()


def start():
    app = QApplication(sys.argv)
    ex = App()
    sys.exit(app.exec_())


